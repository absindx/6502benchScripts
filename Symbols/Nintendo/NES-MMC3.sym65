MMC3_BankNumber		> $8001	;DDDD DDDD
MMC3_Mirroring		> $A000	;---- ---M (0: vertical, 1: horizontal)
MMC3_RamProtect		> $A001	;RWXX ----
MMC3_IrqLatch		> $C000	;DDDD DDDD
MMC3_IrqReload		> $C001	;---- ----
MMC3_IrqDisable		> $E000	;---- ----
MMC3_IrqEnable		> $E001	;---- ----
